{
  "name": "Blur All Whatsapp Web Media",
  "description": "This extension adds blur to all images and videos inside Whatsapp Web, not exposing its content until they are selected.",
  "version": "1.0",
  "manifest_version": 3,
  "content_scripts": [{
    "css": ["blurred-media.css"],
    "matches": ["https://web.whatsapp.com/*"]
  }],
  "action": {
    "default_icon": {
      "16": "/images/blurred_media16.png",
      "32": "/images/blurred_media32.png",
      "48": "/images/blurred_media48.png",
      "128": "/images/blurred_media128.png"
    }
  },
  "icons": {
    "16": "/images/blurred_media16.png",
    "32": "/images/blurred_media32.png",
    "48": "/images/blurred_media48.png",
    "128": "/images/blurred_media128.png"
  }
}